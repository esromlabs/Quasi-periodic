TO t1 :s :l
  ifelse :l < 1 [
    rt 2 * 36
    pendown
    fd :s
    penup
    bk :s
    lt 2 * 36
  ] [
    penup
    fd :s * 0.618034
    rt 3 * 36
    t2 :s * 0.618034 :l - 1
    lt 2 * 36
    fd :s * 0.618034
    rt 3 * 36
    w2 :s * 0.618034 :l - 1
    rt 1 * 36
    fd :s * 0.618034
    rt 1 * 36
    fd :s * 0.618034
    rt 4 * 36
  ]
  penup
END

TO t2 :s :l
  ifelse :l < 1 [
    penup
    rt 2 *36
    fd :s
    lt 4 * 36
    pendown
    fd :s
    penup
    rt 2 *36
    bk :s * 0.618034
  ] [
    penup
    rt 0 * 36
    fd :s * 0.618034
    rt 1 * 36
    fd :s * 0.618034
    rt 3 *36
    w34 :s * 0.618034 :l - 1
    rt 2 * 36
    fd :s * 0.618034
    lt 3 * 36
    t4 :s * 0.618034 :l - 1
    lt 3 * 36
    bk :s * 0.618034
  ]
  penup
END

