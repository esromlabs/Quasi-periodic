TO w34 :s :l
  ifelse :l < 1 [
    penup
    fd :s * 1.618034
    lt 4 * 36
    pendown
    fd :s
    lt 2 * 36
    fd :s
    penup
    lt 4 * 36
  ] [
    penup
    fd :s * 1.618034
    rt 4 * 36
